"use strict"

service rssCon( url = "http://feeds.nytimes.com/nyt/rss/HomePage" ) {
   const feed = require( url, "../../../feed" );

   return <html>
     <h1> ${feed.head.title} </h1>

     ${feed.items.map( i => {
	<div>
	  <h2> ${i.title} </h2>
	  <div> ${i.date} </div>
	  <div> ${i.summary> }</div>
	</div>
     } ) }
   </html>
}

rssCon( "http://rss.slashdot.org/Slashdot/slashdotMain" );
rssCon( "https://www.techradar.com/rss/news/computing" );
